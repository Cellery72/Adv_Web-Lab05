<% include partials/header.ejs %>

<h1>Users</h1>
<table class="table table-striped">
  <tr>
    <th>User ID</th>
    <th>Username</th>
  </tr>
  <% for (var i = 0; i < users.length; i++) { %>
    <tr>
      <td><%= users[i]._id %></td>
      <td><%= users[i].username %></td>
    </tr>
  <% } %>
</table>

